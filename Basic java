import java.util.*;
class Solution{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int reversed=0;
        
        while(num!=0){
            int last=num%10;
            reversed=reversed*10+last;
            num=num/10;
            
        }
        System.out.println(reversed);
    }
}
